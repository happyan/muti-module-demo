<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yx.module.dao.UserDao">

    <select id="getAll" resultType="com.yx.module.model.User">
        select * from user
    </select>


    <select id="getAllItem" resultType="com.yx.module.model.User">
        select * from user_#{index}
    </select>


</mapper>